{
  "expo": {
    "name": "khaodai",
    "slug": "khaodai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cart.png",
    "scheme": "khaodai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": { 
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cart.png",
        "backgroundColor": "#FFBA4D"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.webshot.khaodai"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/main-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/cart.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#FFBA4D"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a79a8ebd-96f4-4f5c-be84-396653bd2b06"
      }
    }
  }
}
